{
  "name": "co-debugger",
  "displayName": "co-debugger",
  "description": "",
  "version": "0.4.0",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "repository": "https://github.com/spideruci/ad-hoc-async",
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "co-debugger.customEditor",
        "displayName": "Custom JS/TS Editor",
        "selector": [
          {
            "filenamePattern": "*.js"
          },
          {
            "filenamePattern": "*.ts"
          }
        ],
        "priority": "default"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "co-debugger-sidebar",
          "title": "Co Debugger",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "co-debugger-sidebar": [
        {
          "type": "webview",
          "id": "co-debugger.sidebarView",
          "name": "Sidebar View"
        }
      ]
    }
  },
  "activationEvents": [
    "onView:co-debugger.sidebarView",
    "*"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "publisher": "SpiderLabAtUCI",
  "scripts": {
    "vscode:prepublish": "rm -rf ./node_modules package-lock.json && npm install --legacy-peer-deps && npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "prepare": "husky",
    "commit": "npx git-cz"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/vscode": "^1.95.0",
    "@types/ws": "^8.5.14",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "commitizen": "^4.3.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "cz-customizable": "^7.4.0",
    "eslint": "^9.13.0",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.7",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "stream-browserify": "^3.0.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.11",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "util": "^0.12.5",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@monaco-editor/react": "^4.6.0",
    "@mui/icons-material": "^6.4.5",
    "@mui/material": "^6.4.5",
    "@typescript-eslint/typescript-estree": "^8.23.0",
    "@use-gesture/react": "^10.3.1",
    "@xyflow/react": "^12.4.2",
    "dnd-timeline": "^2.1.1",
    "express": "^4.21.2",
    "highcharts": "^12.1.2",
    "highcharts-react-official": "^3.2.1",
    "monaco-editor": "^0.52.2",
    "react": "^17.0.0",
    "react-complex-tree": "^2.4.6",
    "react-dom": "^17.0.0",
    "react-json-view": "^1.21.3",
    "react-select": "^5.10.0",
    "recharts": "^2.15.1"
  }
}
